{
	"compilerOptions": {
		"target": "es5",
		"lib": ["es2021", "dom"],
		"module": "commonjs",
		"rootDir": "./src",
		"outDir": "./dist",
		"declaration": true,
		"resolveJsonModule": true,
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true
	},
	"include": ["src"]
}