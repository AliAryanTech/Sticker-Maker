{
  "name": "@shibam/sticker-maker",
  "version": "1.1.5",
  "description": "A package for creating stickers",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p .",
    "lint": "eslint \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "fmt": "prettier --config .prettierrc \"**/*.{ts,mjs}\" --write",
    "test": "mocha --timeout 60000 -r ts-node/register \"tests/**/*.test.ts\"",
    "clean:webp": "find ./ -name '*.webp' -delete",
    "release": "release-it"
  },
  "author": "Shibam Dey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NekoSenpai69/Sticker-Maker.git"
  },
  "keywords": [
    "stickers",
    "sticker maker",
    "image processing",
    "wa sticker maker",
    "wasticker",
    "nodejs",
    "typescript"
  ],
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "file-type": "^19.4.0",
    "fluent-ffmpeg": "^2.1.3",
    "node-webpmux": "^3.2.0",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}
